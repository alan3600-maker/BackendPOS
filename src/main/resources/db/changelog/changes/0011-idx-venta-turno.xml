<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
	xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.29.xsd">

	<changeSet id="0011-idx-venta-turno" author="lmsc">
		<preConditions onFail="MARK_RAN" onError="HALT">
			<not>
				<indexExists tableName="venta" indexName="idx_venta_caja_turno" />
			</not>
		</preConditions>

		<createIndex tableName="venta" indexName="idx_venta_caja_turno">
			<column name="caja_turno_id" />
		</createIndex>
	</changeSet>


</databaseChangeLog>